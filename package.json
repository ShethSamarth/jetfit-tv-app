{
  "name": "jetfit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "tvos": "expo run:ios --scheme jetfit-tvOS --device \"Apple TV\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "build:clean": "cd android && ./gradlew clean && cd ..",
    "build:apk": "cd android && ./gradlew assembleRelease && cd ..",
    "build:bundle": "cd android && ./gradlew bundleRelease && cd .."
  },
  "dependencies": {
    "@react-native-community/slider": "^4.5.3",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "expo": "^48.0.16",
    "react": "18.2.0",
    "react-native": "npm:react-native-tvos@^0.71.10-0",
    "react-native-animatable": "^1.4.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-radial-gradient": "^1.1.5",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-video": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.19.0",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.6.4"
}
